		"devdoc": {
			"details": "Manages user subscriptions with Free, Monthly, and Yearly plans",
			"errors": {
				"OwnableInvalidOwner(address)": [
					{
						"details": "The owner is not a valid owner account. (eg. `address(0)`)"
					}
				],
				"OwnableUnauthorizedAccount(address)": [
					{
						"details": "The caller account is not authorized to perform an operation."
					}
				]
			},
			"kind": "dev",
			"methods": {
				"checkAccess(address)": {
					"details": "Check if user has access to DCA features",
					"params": {
						"user": "Address of the user to check"
					},
					"returns": {
						"_0": "hasAccess Whether user has active subscription"
					}
				},
				"constructor": {
					"details": "Constructor initializes plan prices and durations",
					"params": {
						"_treasury": "Address to receive subscription payments",
						"_usdcToken": "Address of USDC token contract"
					}
				},
				"getOwner(address)": {
					"details": "Get owner of a user's subscription",
					"params": {
						"user": "Address of the user"
					},
					"returns": {
						"_0": "owner Address of the owner"
					}
				},
				"getPlanDetails(uint8)": {
					"details": "Get plan details",
					"params": {
						"planType": "Type of plan"
					},
					"returns": {
						"duration": "Duration of the plan",
						"isActive": "Whether plan is active",
						"price": "Price of the plan"
					}
				},
				"getSubscription(address)": {
					"details": "Get user subscription details",
					"params": {
						"user": "Address of the user"
					},
					"returns": {
						"expiryTimestamp": "When the subscription expires",
						"hasAccess": "Current access status",
						"isExpired": "Whether the subscription has expired",
						"planType": "Current plan type"
					}
				},
				"grantAccess(address,uint8,uint256)": {
					"details": "Grant premium access to a user (only owner, for testing/promotions)",
					"params": {
						"duration": "Duration of access",
						"planType": "Plan type to grant",
						"user": "Address of the user"
					}
				},
				"owner()": {
					"details": "Returns the address of the current owner."
				},
				"purchasePlan(uint8)": {
					"details": "Purchase a subscription plan",
					"params": {
						"planType": "The type of plan to purchase (MONTHLY or YEARLY)"
					}
				},
				"renounceOwnership()": {
					"details": "Leaves the contract without owner. It will not be possible to call `onlyOwner` functions. Can only be called by the current owner. NOTE: Renouncing ownership will leave the contract without an owner, thereby disabling any functionality that is only available to the owner."
				},
				"revokeExpiredAccess(address)": {
					"details": "Revoke access for expired subscriptions (can be called by anyone)",
					"params": {
						"user": "Address of the user to check"
					}
				},
				"setOwner(address)": {
					"details": "Set owner for the calling user's subscription",
					"params": {
						"owner": "Address to set as owner"
					}
				},
				"transferOwnership(address)": {
					"details": "Transfers ownership of the contract to a new account (`newOwner`). Can only be called by the current owner."
				},
				"updatePlan(uint8,uint256,uint256,bool)": {
					"details": "Update plan details (only owner)",
					"params": {
						"duration": "New duration in seconds",
						"isActive": "Whether plan is active",
						"planType": "Type of plan to update",
						"price": "New price in USDC"
					}
				},
				"updateTreasury(address)": {
					"details": "Update treasury address (only owner)",
					"params": {
						"newTreasury": "New treasury address"
					}
				}
			},
